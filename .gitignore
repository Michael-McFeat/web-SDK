# .gitignore for Vue.js projects

# Dependencies
/node_modules
/dist

# Log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Editor directories and files
.idea
.vscode
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw?

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Vue CLI configuration
.env.local
.env.*.local

# Optional: Exclude all .env files if you're using them for secrets
# .env*

# Optional: Cache files
.cache
.npm

# Optional: Coverage directory used by tools like istanbul
/coverage

# Optional: Cypress e2e testing
/cypress/videos/
/cypress/screenshots/

# Optional: Storybook build outputs
/storybook-static

# Optional: PWA files
/public/precache-manifest.*
/public/service-worker.js

# Optional: jsconfig.json for VSCode
jsconfig.json

# Optional: Nuxt.js build output
.nuxt

# Optional: Quasar build output
.quasar
